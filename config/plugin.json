{
	"Q": {
		"pluginInfo": {
			"Telegram": {
				"version": "0.2",
				"compatible": "0.1",
				"requires": {
					"Q": "1.0",
					"Users": "1.0",
					"Streams": "1.0"
				},
				"permissions": [],
				"connections": ["Users", "Telegram"]
			}
		},
		"handlersBeforeEvent": {
			"Q/Request/platform": ["Telegram/before/Q_Request_platform"]
		},
		"handlersAfterEvent": {
			"Q/Plugin/install": ["Telegram/after/Q_Plugin_install"]
		},
		"routes@end": {},
		"routes": {
			"Telegram/authenticate": {"module": "Telegram", "action": "authenticate"}
		},
		"web": {},
		"javascript": {},
		"text": {},
		"images": {},
		"bot": {
			"userAgent": "Qbix"
		},
		"expirations": {
			"telegram": 600
		}
	},
	"Users": {
		"apps": {
			"telegram": {
				"*": {
					"appIdForAuth": "all"
				}
			}
		},
		"apps-private": {
			"telegram": ["token", "secret"]
		},
		"import": {
			"telegram": [
				"first_name", "last_name", "username", "language_code", "photo"
			]
		}
	},
	"Streams": {
		"userStreams": {
			"Telegram": "streams.json"
		}
	}
}
